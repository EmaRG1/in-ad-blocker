{
    "manifest_version": 3,
    "name": "LinkedIn AdBlocker",
    "author":"Emanuel Rojas Guindan",
    "version": "0.0.1",
    "permissions": [
      "tabs",
      "scripting",
      "webNavigation",
      "webRequest",
      "activeTab"
    ],
    "host_permissions": [
      "https://www.linkedin.com/*"
    ],
    "background": {
      "service_worker": "extension.js"
    }
  }
  